
@{
    ViewBag.Title = "Find Blogs";
}

<h2>Blogi zawierające szukane tagi:</h2>

<div id="content-placeholder" class="container" style="margin-top:20px;margin-bottom:20px;"></div>

<script type="text/javascript">
    var url = '/home/GetJSONFindBlogs';

    $(document).ready(function () {
        var jsonData = getContextData();
        var context = $.parseJSON(jsonData);
        var html = Handlebars.templates['post'](context);
        $("#content-placeholder").html(html);
    })
</script>